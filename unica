                    //console.debug('hover:: ', text);
                    dataLayer.push({'event': 'customEvent', 'evCat': 'expandMenu', 'evAct': 'mouseOver', 'evLab': text });
                }
            });
        } else {
            jQuery('.has-submenu-icon').on('click', function(){
